GRMIDX_SRCS += $(addprefix $(GENDIR)/,$(addsuffix .grmidx,$(JHTML_SRCS)))

$(OUTPUT)/grammar-index.html : $(GRMIDX_SRCS) $(SCRIPTDIR)/build-grammar-index.rb
